generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model JobPosting {
  id             String        @id @default(auto()) @map("_id") @db.ObjectId
  name           String
  description    String
  salary         Float?   @default(0)
  location       String
  author         String
  img            String?
  employmentType String?
  createdAt      DateTime @default(now())
}